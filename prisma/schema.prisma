// prisma/schema.prisma
generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Track {
  id        String   @id @default(cuid())
  uid       String
  eventType String
  payload   String?
  createdAt DateTime @default(now())
}

model Content {
  id        String   @id @default(cuid())
  title     String
  body      String
  category  String
  createdAt DateTime @default(now())
}
